{
    "version": 2,
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static"
      },
      {
        "src": "projects.html",
        "use": "@vercel/static"
      },
      {
        "src": ".*",
        "dest": "/index.html",
        "use": "@vercel/redirects"
      }
    ],
    "routes": [
      {
        "src": "/projects",
        "dest": "/projects.html",
        "status": 200
      },
      {
        "src": "/.*",
        "dest": "/index.html"
      }
    ]
  }