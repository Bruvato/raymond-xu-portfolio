{
    "version": 2,
    "builds": [
      { "src": "index.html", "use": "@vercel/static" },
      { "src": "projects.html", "use": "@vercel/static" }
    ],
    "routes": [
      {
        "src": "/projects",
        "dest": "/projects.html",
        "status": 200
      },
      {
        "src": "/.*",
        "dest": "/index.html"
      }
    ]
  }
  